# Production Server Credentials
# Copy this file to .env.prod and fill in real values
# NEVER commit .env.prod to git!

# SSH connection to production server
PROD_SSH_HOST=your_server_ip
PROD_SSH_USER=root

# Production MySQL credentials
PROD_MYSQL_DATABASE=content_fabric
PROD_MYSQL_USER=your_mysql_user
PROD_MYSQL_PASSWORD=your_mysql_password
